# LCSC API Credentials
# Get your API key from: https://www.lcsc.com/agent
LCSC_API_KEY=your_api_key_here
LCSC_API_SECRET=your_api_secret_here

# API Configuration
LCSC_API_BASE_URL=https://www.lcsc.com
LCSC_API_TIMEOUT=30
LCSC_API_MAX_RETRIES=3

# Application Settings
DEFAULT_CURRENCY=USD
DEFAULT_PAGE_SIZE=100
FUZZY_MATCH_THRESHOLD=75
ENABLE_CACHE=true
CACHE_TTL_DAYS=7

# Logging
LOG_LEVEL=INFO
LOG_FILE=lhatolcsc.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5

# GUI Settings
WINDOW_WIDTH=1280
WINDOW_HEIGHT=800
THEME=default

# Performance
MAX_CONCURRENT_REQUESTS=5
REQUEST_DELAY_MS=200
BATCH_SIZE=50

# Development
DEBUG=false
ENABLE_PROFILING=false
